@{
    WhatsApp = @{
        icon           = "whatsapp.png"
        accountPattern = "[0-9]+@s.whatsapp.net"
    }
    Generic = @{
        icon           = "imessage.png"
        accountPattern = ""
    }
    Telegram = @{
        icon           = "telegram.png"
        accountPattern = "[0-9]+"
    }
    iMessage= @{
        icon           = "imessage.png"
        accountPattern = "\+[0-9]+"
    }
}